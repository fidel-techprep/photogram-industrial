<div class="row">
  <div class="col md-6 offset-3">
    <h1>
      <%= @user.username %>
    </h1>

    <%= render partial: "shared/tabs" %>
  </div>

</div>
<div class="row mb-4">
  <div class="col-md-6 offset-md-3">
    <div class="list-group">
      <% users_list.each do |user| %>
        <li class="list-group-item"><%= link_to user.username, user_path(user.username) %></li>
      <% end %>
    </div>
  </div>
</div>
